    <engine:VisualElement name="Root" style="width: 100%; height: 100%; -unity-background-image-tint-color: rgba(255, 255, 255, 0.5); background-image: none; -unity-background-scale-mode: scale-and-crop; flex-grow: 1; flex-shrink: 1; align-content: flex-start; flex-direction: column;">
